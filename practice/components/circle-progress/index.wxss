/* pages/circle-progress.wxss */
.canvasBox {
  position: relative;
  height: 500rpx;
  background-color: #ffffff;
}

/* 外部灰色的圆 */
.bigCircle {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 420rpx;
  height: 420rpx;
  margin: auto auto;
  border-radius: 50%;
  background-color: #f2f2f2;
}

/* 内部白色的圆 */
.littleCircle {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 350rpx;
  height: 350rpx;
  margin: auto auto;
  border-radius: 50%;
  background-color: white;
}

.canvas {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 420rpx;
  height: 420rpx;
  margin: auto auto;
  z-index: 99;
}